<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <system.web>
      <authorization>
         <allow users="*" />
         <deny users="?" />
      </authorization>
   </system.web>
   <system.webServer>
      <httpProtocol>
          <customHeaders>
	    <add name="X-UA-Compatible" value="IE=Edge" />
	    <add name="Access-Control-Allow-Origin" value="*" />
          </customHeaders>
      </httpProtocol>
  </system.webServer>
</configuration>